<appendix xml:id="xmlreader.constants" xmlns="http://docbook.org/ns/docbook">
 &reftitle.constants;
 &extension.constants;
  <warning>
   <para>
    Od PHP 5.1 XMLReader używa stałych zdefiniowanych w klasie.
    We wcześniejszych wydaniach należy stosować stałe globalne
    <constant>XMLREADER_ELEMENT</constant>.
   </para>
  </warning>
  <table>
   <title>Typy węzłów XMLReader</title>
   <tgroup cols="3">
    <thead>
     <row>
      <entry>Stała</entry>
      <entry>Wartość</entry>
      <entry>Opis</entry>
     </row>
    </thead>
    <tbody>
     <row xml:id="xmlreader.constants.none">
      <entry>
       <constant>XMLReader::NONE</constant>
       (<type>integer</type>)
      </entry>
      <entry>0</entry>
      <entry>brak typu</entry>
     </row>
     <row xml:id="xmlreader.constants.element">
      <entry>
       <constant>XMLReader::ELEMENT</constant>
       (<type>integer</type>)
      </entry>
      <entry>1</entry>
      <entry>element otwierający</entry>
     </row>
     <row xml:id="xmlreader.constants.attribute">
      <entry>
       <constant>XMLReader::ATTRIBUTE</constant>
      (<type>integer</type>)
      </entry>
      <entry>2</entry>
      <entry>atrybut</entry>
     </row>
     <row xml:id="xmlreader.constants.text">
      <entry>
       <constant>XMLReader::TEXT</constant>
       (<type>integer</type>)
      </entry>
      <entry>3</entry>
      <entry>węzeł tekstowy</entry>
     </row>
     <row xml:id="xmlreader.constants.cdata">
      <entry>
       <constant>XMLReader::CDATA</constant>
       (<type>integer</type>)
      </entry>
      <entry>4</entry>
      <entry>CDATA</entry>
     </row>
     <row xml:id="xmlreader.constants.entity-ref">
      <entry>
       <constant>XMLReader::ENTITY_REF</constant>
       (<type>integer</type>)
      </entry>
      <entry>5</entry>
      <entry>referencja do encji</entry>
     </row>
     <row xml:id="xmlreader.constants.entity">
      <entry>
       <constant>XMLReader::ENTITY</constant>
       (<type>integer</type>)
      </entry>
      <entry>6</entry>
      <entry>deklaracja encji</entry>
     </row>
     <row xml:id="xmlreader.constants.pi">
      <entry>
       <constant>XMLReader::PI</constant>
       (<type>integer</type>)
      </entry>
      <entry>7</entry>
      <entry>instrukcja przetwarzania</entry>
     </row>
     <row xml:id="xmlreader.constants.comment">
      <entry>
       <constant>XMLReader::COMMENT</constant>
       (<type>integer</type>)
      </entry>
      <entry>8</entry>
      <entry>komentarz</entry>
     </row>
     <row xml:id="xmlreader.constants.doc">
      <entry>
       <constant>XMLReader::DOC</constant>
       (<type>integer</type>)
      </entry>
      <entry>9</entry>
      <entry>dokument</entry>
     </row>
     <row xml:id="xmlreader.constants.doc-type">
      <entry>
       <constant>XMLReader::DOC_TYPE</constant>
       (<type>integer</type>)
      </entry>
      <entry>10</entry>
      <entry>typ dokumentu</entry>
     </row>
     <row xml:id="xmlreader.constants.doc-fragment">
      <entry>
       <constant>XMLReader::DOC_FRAGMENT</constant>
       (<type>integer</type>)
      </entry>
      <entry>11</entry>
      <entry>fragment dokumentu</entry>
     </row>
     <row xml:id="xmlreader.constants.notation">
      <entry>
       <constant>XMLReader::NOTATION</constant>
       (<type>integer</type>)
      </entry>
      <entry>12</entry>
      <entry>notacja</entry>
     </row>
     <row xml:id="xmlreader.constants.whitespace">
      <entry>
       <constant>XMLReader::WHITESPACE</constant>
       (<type>integer</type>)
      </entry>
      <entry>13</entry>
      <entry>biały znak</entry>
     </row>
     <row xml:id="xmlreader.constants.significant-whitespace">
      <entry>
       <constant>XMLReader::SIGNIFICANT_WHITESPACE</constant>
       (<type>integer</type>)
      </entry>
      <entry>14</entry>
      <entry>znaczący biały znak</entry>
     </row>
     <row xml:id="xmlreader.constants.end-element">
      <entry>
       <constant>XMLReader::END_ELEMENT</constant>
       (<type>integer</type>)
      </entry>
      <entry>15</entry>
      <entry>element zamykający</entry>
     </row>
     <row xml:id="xmlreader.constants.end-entity">
      <entry>
       <constant>XMLReader::END_ENTITY</constant>
       (<type>integer</type>)
      </entry>
      <entry>16</entry>
      <entry>koniec encji</entry>
     </row>
     <row xml:id="xmlreader.constants.xml-declaration">
      <entry>
       <constant>XMLReader::XML_DECLARATION</constant>
       (<type>integer</type>)
      </entry>
      <entry>17</entry>
      <entry>deklaracja XML</entry>
     </row>
    </tbody>
   </tgroup>
  </table>
  <table>
   <title>Opcje parsera XMLReader</title>
   <tgroup cols="3">
    <thead>
     <row>
      <entry>Stała</entry>
      <entry>Wartość</entry>
      <entry>Opis</entry>
     </row>
    </thead>
    <tbody>
     <row xml:id="xmlreader.constants.loaddtd">
      <entry>
       <constant>XMLReader::LOADDTD</constant>
       (<type>integer</type>)
      </entry>
      <entry>1</entry>
      <entry>Wczytuje DTD, ale nie waliduje</entry>
     </row>
     <row xml:id="xmlreader.constants.defaultattrs">
      <entry>
       <constant>XMLReader::DEFAULTATTRS</constant>
       (<type>integer</type>)
      </entry>
      <entry>2</entry>
      <entry>Wczytuje DTD i domyślne atrybuty, ale nie waliduje</entry>
     </row>
     <row xml:id="xmlreader.constants.validate">
      <entry>
       <constant>XMLReader::VALIDATE</constant>
       (<type>integer</type>)
      </entry>
      <entry>3</entry>
      <entry>Wczytuje DTD i waliduje podczas parsowania</entry>
     </row>
     <row xml:id="xmlreader.constants.entities">
      <entry>
       <constant>XMLReader::SUBST_ENTITIES</constant>
       (<type>integer</type>)
      </entry>
      <entry>4</entry>
      <entry>Zastępuje encje i rozszerza referencje</entry>
     </row>
    </tbody>
   </tgroup>
  </table>
</appendix>

