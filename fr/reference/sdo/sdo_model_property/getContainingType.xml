<refentry xml:id="sdo-model-property.getcontainingtype" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>SDO_Model_Property::getContainingType</refname>
  <refpurpose>
   Récupère le SDO_Model_Type qui contient cette propriété
  </refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>SDO_Model_Type</type><methodname>SDO_Model_Property::getContainingType</methodname>
   <void/>
  </methodsynopsis>

  &warn.experimental.func;

  <para>
   Récupère le SDO_Model_Type qui contient cette propriété.
  </para>

 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   Aucun.
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne le SDO_Model_Type qui contient cette propriété.
  </para>
 </refsect1>

</refentry>

