<refentry xml:id="function.gc-enabled" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>gc_enabled</refname>
  <refpurpose>Retourne le statut du collecteur de références circulaires</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>gc_enabled</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Retourne le statut du collecteur de références circulaires.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>
 
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne &true; si le collecteur est actif, et &false; sinon.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Exemple avec <function>gc_enabled</function></title>
    <programlisting role="php">
<![CDATA[
<?php
if(gc_enabled()) gc_collect_cycles();
?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><link linkend="features.gc">Le collecteur</link></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

