<refentry xml:id="function.wddx-add-vars" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>wddx_add_vars</refname>
  <refpurpose>Tanıtıcısı belirtilen WDDX paketine değişken ekler</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>wddx_add_vars</methodname>
   <methodparam><type>resource</type><parameter>pt</parameter></methodparam>
   <methodparam><type>mixed</type><parameter>değişken</parameter></methodparam>
   <methodparam choice="opt"><type>mixed</type><parameter>...</parameter></methodparam>
  </methodsynopsis>
  <para>
   Aktarılan değişkenleri dizgeleştirip tanıtıcısı belirtilen WDDX paketine
   ekler.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   Bu işlevin alabileceği değiştirge sayısı değişkendir.
   <variablelist>
    <varlistentry>
     <term><parameter>pt</parameter></term>
     <listitem>
      <para>
       <function>wddx_packet_start</function> tarafından döndürülmüş bir WDDX
       paket tanıtıcısı.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>değişken</parameter></term>
     <listitem>
      <para>
       Bir değişken ismi veya değişken isimlerini veya diğer dizileri ve
       benzerlerini içeren bir dizi olabilir.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>...</parameter></term>
     <listitem>
      <para>
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>
</refentry>

