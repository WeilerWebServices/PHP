<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.imagick-addnoiseimage">
 <refnamediv>
  <refname>Imagick::addNoiseImage</refname>
  <refpurpose>Resme rasgele gürültü ekler</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>Imagick::addNoiseImage</methodname>
   <methodparam><type>int</type><parameter>gürültü_türü</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>kanal</parameter><initializer>Imagick::CHANNEL_DEFAULT</initializer></methodparam>
  </methodsynopsis>
  &warn.undocumented.func;
  <para>
   Resme rasgele gürültü ekler.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>gürültü_türü</parameter></term>
     <listitem>
      <para>
       <link linkend="imagick.constants.noise">Gürültü türü
       sabitlerinden</link> biri.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>kanal</parameter></term>
     <listitem>
      <para>&imagick.parameter.channel;</para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>

 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &imagick.return.success;
  </para>
 </refsect1>

</refentry>

