<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.imagick-newpseudoimage">
 <refnamediv>
  <refname>Imagick::newPseudoImage</refname>
  <refpurpose>Yeni bir resim oluşturur</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>Imagick::newPseudoImage</methodname>
   <methodparam><type>int</type><parameter>genişlik</parameter></methodparam>
   <methodparam><type>int</type><parameter>yükseklik</parameter></methodparam>
   <methodparam><type>string</type><parameter>sözde_dizge</parameter></methodparam>
  </methodsynopsis>
  &warn.undocumented.func;
  <para>
   <classname>ImageMagick</classname> sözde biçemlerini kullanarak yeni bir
   resim oluşturur.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>genişlik</parameter></term>
     <listitem>
      <para>
       Yeni resmin genişliği.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>yükseklik</parameter></term>
     <listitem>
      <para>
       Yeni resmin yüksekliği
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>sözde_dizge</parameter></term>
     <listitem>
      <para>
       Sözde resim tanımını içeren dizge.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>

 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &imagick.return.success;
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   &imagick.imagick.throws;
  </para>
 </refsect1>

</refentry>

