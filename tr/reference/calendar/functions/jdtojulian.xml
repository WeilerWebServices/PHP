<refentry xml:id="function.jdtojulian" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>JDToJulian</refname>
  <refpurpose>Jülyen Gün Sayısını, Jülyen takvim tarihine çevirir</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>jdtojulian</methodname>
   <methodparam><type>int</type><parameter>julyengunu</parameter></methodparam>
  </methodsynopsis>
  <para>
   Jülyen Gün Sayısını, "ay/gün/yıl" biçiminde Jülyen takvim tarihine çevirir.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>julyengunu</parameter></term>
     <listitem>
      <para>
       Tamsayı olarak Jülyen Gün Sayısı
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   "ay/gün/yıl" biçiminde dizge olarak Jülyen takvimi tarihi
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>juliantojd</function></member>
    <member><function>cal_from_jd</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

