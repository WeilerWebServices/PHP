<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.utf8-decode">
 <refnamediv>
  <refname>utf8_decode</refname>
  <refpurpose>ISO-8859-1 karakterlerinin UTF-8 gösterimlerini ISO-8859-1
  gösterimlerine dönüştürür</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>utf8_decode</methodname>
   <methodparam><type>string</type><parameter>veri</parameter></methodparam>
  </methodsynopsis>
  <para>
   Belirtilen <parameter>data</parameter> dizgesindeki
   <literal>ISO-8859-1</literal> karakterlerinin <literal>UTF-8</literal>
   gösterimlerini <literal>ISO-8859-1</literal> gösterimlerine dönüştürür.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>veri</parameter></term>
     <listitem>
      <para>
       UTF-8 ile kodlanmış bir dizge.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Belirtilen <parameter>veri</parameter> dizgesinin ISO-8859-1 gösterimini
   döndürür.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>utf8_encode</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

