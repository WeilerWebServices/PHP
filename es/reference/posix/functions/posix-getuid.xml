<refentry xml:id="function.posix-getuid" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>posix_getuid</refname>
  <refpurpose>Devolver el ID real de usuario del proceso actual</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>posix_getuid</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Devuelve el ID real num√©rico de usuario del proceso actual.
  </para>
 </refsect1>
 
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Devuelve el id de usuario, como valor de tipo <type>integer</type>
  </para>
 </refsect1>
 
 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Ejemplo de uso de <function>posix_getuid</function></title>
    <programlisting role="php">
<![CDATA[
<?php
echo posix_getuid(); //10000
?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>
 
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member>
     <function>posix_getpwuid</function></member>
    <member>POSIX man page GETUID(2)</member>
   </simplelist>
  </para>
 </refsect1>
 
</refentry>

