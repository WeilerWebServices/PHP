<refentry xmlns="http://docbook.org/ns/docbook" xml:id="imagick.getimagetotalinkdensity">
 <refnamediv>
  <refname>Imagick::getImageTotalInkDensity</refname>
  <refpurpose>Obtiene la densidad de tinta total de la imagen</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>float</type><methodname>Imagick::getImageTotalInkDensity</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Obtiene la densidad de tinta total de la imagen.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
    Devuelve la densidad de tinta total de la imagen.
    &imagick.imagickexception.throw;
  </para>
 </refsect1>

</refentry>

