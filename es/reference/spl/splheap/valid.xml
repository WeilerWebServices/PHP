<refentry xml:id="splheap.valid" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>SplHeap::valid</refname>
  <refpurpose>Comprueba si el montón contiene más nodos</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>bool</type><methodname>SplHeap::valid</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Comprueba si el montón contiene más nodos.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Devuelve &true; si el montón contiene más nodos, en caso contrario &false;.
  </para>
 </refsect1>

</refentry>

