<refentry xml:id="function.newt-clear-key-buffer" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>newt_clear_key_buffer</refname>
  <refpurpose>Desecha el contenido del búfer de entrada del terminal sin
   esperar a entradas adicionales</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>void</type><methodname>newt_clear_key_buffer</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Desecha el contenido del búfer de entrada del terminal sin esperar
   a entradas adicionales,
  </para>
 </refsect1>
 
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>
 
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>newt_wait_for_key</function></member>
   </simplelist>
  </para>
 </refsect1>
 
</refentry>

