<refentry xml:id="resourcebundle.count" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ResourceBundle::count</refname>
  <refname>resourcebundle_count</refname>
  <refpurpose>Obtener el número de elementos de un paquete</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <para>
   &style.oop;
  </para>
  <methodsynopsis>
   <modifier>public</modifier>
   <type>int</type>
   <methodname>ResourceBundle::count</methodname>
   <void />
  </methodsynopsis>
  <para>
   &style.procedural;
  </para>
  <methodsynopsis>
   <type>int</type>
   <methodname>resourcebundle_count</methodname>
   <methodparam><type>ResourceBundle</type><parameter>r</parameter></methodparam>
  </methodsynopsis>
  <para>
   Obtiene el número de elementos de un paquete.
  </para>
 </refsect1>
 
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>r</parameter></term>
     <listitem>
      <para>
       Un objeto de la clase <classname>ResourceBundle</classname>.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 
 
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Devuelve el número de elementos de un paquete.
  </para>
 </refsect1>
 
 <refsect1 role="examples">
  &reftitle.examples;
  <example>
   <title>Ejemplo de <function>resourcebundle_count</function></title>
   <programlisting role="php">
<![CDATA[
<?php
$r = resourcebundle_create( 'es', "/usr/share/data/myapp");
echo resourcebundle_count($r);
?>
]]>
   </programlisting>
  </example>
  <example>
   <title>Ejemplo orientado a objetos</title>
   <programlisting role="php">
<![CDATA[
<?php
$r = new ResourceBundle( 'es', "/usr/share/data/myapp");
echo $r->count();
?>
]]>
   </programlisting>
   &example.outputs;
   <screen>
<![CDATA[
42
]]>
   </screen>
  </example>
 </refsect1>
 
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>resourcebundle_get</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

