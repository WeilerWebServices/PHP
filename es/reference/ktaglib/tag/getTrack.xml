<refentry xml:id="tag.gettrack" xmlns="http://docbook.org/ns/docbook">
  <refnamediv>
    <refname>KTaglib_Tag::getTrack</refname>
    <refpurpose>Devuelve el número de pista de una etiqueta ID3</refpurpose>
  </refnamediv>
  <refsect1 role="description">
    &reftitle.description;
    <methodsynopsis>
      <modifier>public</modifier>
      <type>int</type>
      <methodname>KTaglib_Tag::getTrack</methodname>
      <void />
    </methodsynopsis>
    <para>
      Devuelve el número de pista en una etiqueta ID3. Este método es implementado
      en etiquetas ID3v1 y ID3v2.
    </para>
  </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
    Devuelve el número de pista como un entero
  </para>
 </refsect1>

</refentry>

