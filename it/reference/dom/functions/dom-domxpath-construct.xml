<refentry xml:id='function.dom-domxpath-construct' xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>DOMXPath->__construct()</refname>
  <refpurpose>
   Crea un nuovo oggetto DOMXPath
  </refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <classsynopsis>
  <ooclass><classname>DOMXPath</classname></ooclass>
   <constructorsynopsis>
    <methodname>__construct</methodname>
    <methodparam><type>DOMDocument</type><parameter>doc</parameter></methodparam>
   </constructorsynopsis> 
  </classsynopsis>
  <para>
   Crea un nuovo oggetto <classname>DOMXPath</classname>.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>doc</parameter></term>
     <listitem>
      <para>
       L'oggetto <classname>DOMDocument</classname> associato a
       <classname>DOMXPath</classname>.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
</refentry>

