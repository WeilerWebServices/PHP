  <refentry xml:id='function.domdocumenttype-name' xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>DomDocumentType->name</refname>
    <refpurpose>
     Restituisce il nome del tipo documento
    </refpurpose>
   </refnamediv>
   <refsect1>
     &reftitle.description;
    <methodsynopsis>
     <type>string</type><methodname>DomDocumentType->name</methodname>
     <void/>
    </methodsynopsis>
    <para>
     Questa funzione restituisce il nome del tipo documento.
    </para>
    <para>
     <example>
      <title>Ottenere il nome del tipo documento</title>
      <programlisting role="php">
<![CDATA[
<?php
 
include("example.inc");
 
if (!$dom = domxml_open_mem($xmlstr)) {
    echo "Error while parsing the document\n";
    exit;
}
 
$doctype = $dom->doctype();
echo $doctype->name(); // chapter
 
?>
]]>
      </programlisting>
     </example>
    </para>
   </refsect1>
  </refentry>

