  <refentry xml:id="function.bcmod" xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>bcmod</refname>
    <refpurpose>
     Ricava il modulo di un numero a precisione arbitraria
    </refpurpose>
   </refnamediv>
   <refsect1>
    <title>Descrizione</title>
     <methodsynopsis>
      <type>string</type><methodname>bcmod</methodname>
      <methodparam><type>string</type><parameter>operando</parameter></methodparam>
      <methodparam><type>string</type><parameter>modulo</parameter></methodparam>
     </methodsynopsis>
    <para>
     Ricava il modulo di <parameter>operando</parameter> usando
     <parameter>modulo</parameter>.
    </para>
   </refsect1>
   <refsect1>
    &reftitle.examples;
    <example>
     <title>esempio di <function>bcmod</function></title>
     <programlisting role="php">
<![CDATA[
<?php
echo bcmod(4, 2); // 0
echo bcmod(2, 4); // 2
?>
]]>
     </programlisting>
    </example>
   </refsect1>
   <refsect1>
    &reftitle.seealso;
    <para>
     <function>bcdiv</function>.
    </para>
   </refsect1>
  </refentry>

