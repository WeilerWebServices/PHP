<?xml version="1.0" encoding="utf-8"?>
<!-- $Revision: 1.10 $ -->
<classentry id="gtk.gtkcellrendererpixbuf">
 <classmeta>
  <classtitle>GtkCellRendererPixbuf</classtitle>
  <classparent>GtkCellRenderer</classparent>
  <shortdesc>
   Displays a GdkPixbuf in a cell of a GtkTreeView.
  </shortdesc>
  <desc>

  </desc>
 </classmeta>

 <constructors>

  <constructor id="gtk.gtkcellrendererpixbuf.constructor">
   <funcsynopsis>
    <funcprototype>
    <funcdef> <function>GtkCellRendererPixbuf</function></funcdef>
    <paramdef>void</paramdef>
    </funcprototype>
   </funcsynopsis>
   <shortdesc>
    Creates a new <classname>GtkCellRendererPixbuf</classname>.
   </shortdesc>
   <desc>
    <para>
        Creates a new <classname>GtkCellRendererPixbuf</classname>.
        Rendering parameters are adjusted using the object properties.
        The object properties can be set globally with (<function class="GObject">set_property</function>).
        Also, with <classname>GtkTreeViewColumn</classname>, you can bind
        the "pixbuf" property on the cell renderer to a value in a <classname>GtkTreeModel</classname>,
        thus rendering a different image in each row of the <classname>GtkTreeView</classname>.
    </para>
   </desc>
  </constructor>

 </constructors>

<properties>

  <prop id="gtk.gtkcellrendererpixbuf.property.pixbuf" type="rw">
   <propname>pixbuf</propname>
   <proptype>GdkPixbuf</proptype>
   <shortdesc>
    The GdkPixbuf Image to render on the cell.
   </shortdesc>
   <desc>
    <para>
     The GdkPixbuf Image to render on the cell.
    </para>
    <para>
     <example>
      <title>set_property sample</title>
      <programlisting role="php"><![CDATA[<?php
$cellrenderer->set_property('pixbuf',GdkPixbuf::new_from_file('gnome.jpg'));
?>]]></programlisting>
     </example>
    </para>
    <para>
     &seealso;
     <function class="GObject">set_property</function>
    </para>
   </desc>
  </prop>

  <prop id="gtk.gtkcellrendererpixbuf.property.pixbuf-expander-open" type="rw">
   <propname>pixbuf-expander-open</propname>
   <proptype>GdkPixbuf</proptype>
   <shortdesc>
     Pixbuf for open expander.
   </shortdesc>
   <desc>
    <para>
       Pixbuf for open expander.
    </para>
    <para>
     <example>
      <title>set_property sample</title>
      <programlisting role="php"><![CDATA[<?php
$cellrenderer->set_property('pixbuf-expander-open', GdkPixbuf::new_from_file('icone.jpg'));
?>]]></programlisting>
     </example>
    </para>
    <para>
     &seealso;
     <function class="GObject">set_property</function>
    </para>
   </desc>
  </prop>

  <prop id="gtk.gtkcellrendererpixbuf.property.pixbuf-expander-closed" type="rw">
   <propname>pixbuf-expander-closed</propname>
   <proptype>GdkPixbuf</proptype>
   <shortdesc>
     Pixbuf for close expander.
   </shortdesc>
   <desc>
    <para>
       Pixbuf for close expander.
    </para>
    <para>
     <example>
      <title>set_property sample</title>
      <programlisting role="php"><![CDATA[<?php
$cellrenderer->set_property('pixbuf-expander-closed', GdkPixbuf::new_from_file('icone.jpg'));
?>]]></programlisting>
     </example>
    </para>
    <para>
     &seealso;
     <function class="GObject">set_property</function>
    </para>
   </desc>
  </prop>

  <prop id="gtk.gtkcellrendererpixbuf.property.stock-id" type="rw">
   <propname>stock-id</propname>
   <proptype>string</proptype>
   <shortdesc>
    The stock ID of the stock icon to render
   </shortdesc>
   <desc>
    <para>
      The stock ID of the stock icon to render
    </para>
    <para>
     <example>
      <title>set_property sample</title>
      <programlisting role="php"><![CDATA[<?php
$cellrenderer->set_property('stock-id','gtk-add');
?>]]></programlisting>
     </example>
    </para>
    <para>
     &seealso;
     <function class="GObject">set_property</function>
    </para>
   </desc>
  </prop>

  <prop id="gtk.gtkcellrendererpixbuf.property.stock-size" type="rw">
   <propname>stock-size</propname>
   <proptype>integer</proptype>
   <shortdesc>
    The stock ID of the stock icon to render
   </shortdesc>
   <desc>
    <para>
      The stock ID of the stock icon to render
    </para>
    <para>
     <example>
      <title>set_property sample</title>
      <programlisting role="php"><![CDATA[<?php
$cellrenderer->set_property('stock-size',Gtk::ICON_SIZE_BUTTON);
?>]]></programlisting>
     </example>
    </para>
    <para>
     &seealso;
     <function class="GObject">set_property</function>
    </para>
   </desc>
  </prop>
</properties>
</classentry>
